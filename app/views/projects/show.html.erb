<%= link_to 'Back', user_projects_path(@project.user)%>

<div class="container">
<h1><%= @project.title %> Project </h1>
 
 
<p><%= @project.description %></p>

<img src="<%=(url_for(@project.image))%> ">
<br><br>
<% if owner?%>
    <%= link_to 'Edit/Update Your Project', edit_project_path(@project)%><br>
    <%= link_to 'Delete Project', project_path(@project), method: :delete, data: { confirm: 'Are you certain you want to delete this?' }%>
<% end%>

</div>


