<h1>Welcome! Create an account:</h1>
<%= form_for @user, url: '/signup' do |f|%>
    <%= f.label :username%>
    <%= f.text_field :username%>
    <br>
    <%= f.label :password%>
    <%= f.password_field :password%>
    <br><br>
    <%= f.submit%>

<%end%>
<br><br>
<%= link_to('Sign up with Facebook!', '/auth/facebook') %>